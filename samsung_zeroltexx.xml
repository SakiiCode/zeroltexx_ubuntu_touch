<?xml version="1.0" encoding="UTF-8"?>

<manifest>

	<!-- Remotes -->
	<remote name="lineage"
            	fetch="https://github.com/LineageOS"
            	revision="cm-14.1" />
    	<remote name="themuppets"
	    	fetch="https://github.com/TheMuppets"
	    	revision="cm-14.1" />

	<!-- DTs -->
	<project path="device/samsung/zero-common" name="android_device_samsung_zero-common" remote="lineage" />
	<project path="device/samsung/zeroltexx" name="android_device_samsung_zeroltexx" remote="lineage" />


	<!-- Kernel -->
	<project path="kernel/samsung/exynos7420" name="android_kernel_samsung_exynos7420" remote="lineage" />

	<!-- Vendor files -->
	<project path="vendor/samsung" name="proprietary_vendor_samsung" remote="themuppets" />

	<!-- Hardware -->
	<project path="hardware/samsung" name="android_hardware_samsung" remote="lineage" />
    	<project path="hardware/samsung_slsi-cm/exynos" name="android_hardware_samsung_slsi-cm_exynos" remote="lineage" />
    	<project path="hardware/samsung_slsi-cm/exynos5" name="android_hardware_samsung_slsi-cm_exynos5" remote="lineage" />
    	<project path="hardware/samsung_slsi-cm/exynos7420" name="android_hardware_samsung_slsi-cm_exynos7420" remote="lineage" />
    	<project path="hardware/samsung_slsi-cm/openmax" name="android_hardware_samsung_slsi-cm_openmax" remote="lineage" />

</manifest>
